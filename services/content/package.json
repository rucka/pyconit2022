  "scripts": {
    "ts:check": "tsc --skipLibCheck --noEmit",
    "clean": "rm -rf dist",
    "build": "tsc --build --force",
    "start": "ts-node src/index.ts",
    "prettier:fix": "prettier \"{**/*,*}.{ts,tsx,js,jsx,json,html}\" --write",
    "prettier:check": "prettier --list-different \"{**/*,*}.{ts,tsx,js,jsx,json,html,md}\""
  },
  "dependencies": {
    "@pyconit2022/ts-shared": "1.0.0",
    "restify": "^8.6.1"
  },
  "devDependencies": {
    "@types/restify": "^8.5.4",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
